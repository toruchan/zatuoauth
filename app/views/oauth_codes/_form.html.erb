<%= form_for(oauth_code) do |f| %>
  <% if oauth_code.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(oauth_code.errors.count, "error") %> prohibited this oauth_code from being saved:</h2>

      <ul>
      <% oauth_code.errors.full_messages.each do |message| %>
        <li><%= message %></li>
      <% end %>
      </ul>
    </div>
  <% end %>

  <div class="field">
    <%= f.label :user_id %>
